/* rails-erb-loader-dependencies components/ ../test/components/previews/ */

const stories = [];
let story;

<% ActionView::Component::Preview.all.each do |preview| %>
  story = { default: { title: "<%= preview.preview_name.titleize %>" } };

    <% preview.examples.each do |example| %>
      story["<%= example %>"] = () => '<iframe src="/rails/components/<%= preview.preview_name %>/<%= example %>" frameborder="0" style="width:100%;"></iframe>'
    <% end %>

  stories.push(story);

  console.log("built story")
  console.log(story)

<% end %>

export default stories;
